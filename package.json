{
  "name": "angular",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/forms": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/router": "^8.0.0",
    "core-js": "2",
    "lodash": "4.17.15",
    "ngx-json-viewer": "^2.4.0",
    "rxjs": "^6.5.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "zone.js": "^0.9.1"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "replace": "replace-in-file --configFile=script-config/convert-to-dictionary",
    "pretty": "prettier --write ./projects/dotrez-api/src/lib/model/*.ts",
    "build:dotrez-api": "ng build dotrez-api",
    "swagger-gen": "java -jar ./swagger-gen/swagger-codegen-cli.jar generate -i ./swagger-gen/swagger.json -l typescript-angular -o projects/dotrez-api/src/lib/ -t ./swagger-gen/typescript-angular/ -c ./swagger-gen/config.json",
    "gen": "npm run swagger-gen && npm run replace && npm run barrel:api && npm run pretty && npm run build:dotrez-api",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer dist/app-usin-api/stats.json",
    "analyze:dev": "ng build --stats-json && webpack-bundle-analyzer dist/app-usin-api/stats.json",
    "barrel:api": "barrelsby --directory ./projects/dotrez-api/src/lib/model/ --delete --singleQuotes --location all --exclude \"spec.ts|test.data.ts|test.module.ts|internal\\..*?ts\""
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.0.2",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1",
    "prettier": "^1.19.1",
    "replace-in-file": "^5.0.2",
    "barrelsby": "^2.2.0"
  }
}
